{block title}Bankovní účty{/block}
{block content}
<div class="panel panel-default">
    <div class="panel-heading">Přidat účet</div>
    <div class="panel-body">
        {control form}
    </div>
</div>

<h1>Bankovní účty</h1>
<div class="panel panel-default" n:foreach="$accounts as $account">
    <div class="panel-heading">{$account->getName()}</div>
    <div class="panel-body">
        <ul>
            <li>Číslo účtu: {$account->getNumber()}</li>
        </ul>
    </div>
</div>
